@model inmobiliaria.Models.Pago

@{
    ViewData["Title"] = "Eliminar Pago";
}

<h1>Eliminar</h1>

<h3 class="display-6">Esta seguro que quiere eliminar el Pago?</h3>
<div>
    <h4>Pago</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.numPago)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.numPago)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ContratoId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContratoId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.fechaPago)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.fechaPago)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.importe)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.importe)
        </dd>
    </dl>
    
    <form asp-action="Eliminar">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
